<main>
  <h2>List of Users</h2>
 <table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <%@users.each do |user|%>
  <tbody>
      <tr>
        <td><%= link_to user.name, user_path(user.id)%></td>
        <td><%=user.votes.count%></td>
        <td><%=user.created_at.strftime("%m/%d/%Y")%></td>
      </tr>
  </tbody>
  <%end%>
 </table>
 <%= link_to 'Back to Media List', root_path, class: "btn btn-secondary" %>
</main>


